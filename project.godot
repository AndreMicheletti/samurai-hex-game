; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "CardResource",
"language": "GDScript",
"path": "res://scripts/resources/CardResource.gd"
}, {
"base": "Node2D",
"class": "Character",
"language": "GDScript",
"path": "res://scripts/Character.gd"
}, {
"base": "Resource",
"class": "ClassResource",
"language": "GDScript",
"path": "res://scripts/resources/ClassResource.gd"
}, {
"base": "Resource",
"class": "DeckResource",
"language": "GDScript",
"path": "res://scripts/resources/DeckResource.gd"
}, {
"base": "Node2D",
"class": "Tile",
"language": "GDScript",
"path": "res://scripts/Tile.gd"
} ]
_global_script_class_icons={
"CardResource": "",
"Character": "",
"ClassResource": "",
"DeckResource": "",
"Tile": ""
}

[application]

config/name="Samurai Hex"
run/main_scene="res://scenes/TitleScreen.tscn"
config/icon="res://graphics/characters/monk.png"

[autoload]

global="*res://global.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=800
window/size/height=450
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true
